    "def mult(n,m):\n",
    "    if n==0 or m==0:\n",
    "        return 0\n",
    "    if n==1:\n",
    "        return m\n",
    "    if m==1:\n",
    "        return n\n",
    "    return n+mult(n,m-1)\n",
    "\n",
    "n= int(input())\n",
    "m= int(input())\n",
    "mult(n,m)"
   ]
  }
