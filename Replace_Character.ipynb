    "def replace_char(s,a,b):\n",
    "    l = len(s)\n",
    "    if l==0:\n",
    "        return s\n",
    "    smallOutput = replace_char(s[1:],a,b)\n",
    "    if s[0]==a:\n",
    "        return b+smallOutput\n",
    "    else:\n",
    "        return s[0]+smallOutput\n",
    "    \n",
    "s = input()\n",
    "a = input()\n",
    "b = input()\n",
    "replace_char(s,a,b)\n"
   ]
  }
