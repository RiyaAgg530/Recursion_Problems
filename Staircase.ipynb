    "def staircase(n):\n",
    "    if n<=3:\n",
    "        if n==1:\n",
    "            return 1\n",
    "        elif n==2:\n",
    "            return 2\n",
    "        else:\n",
    "            return 4\n",
    "    x = staircase(n-1)\n",
    "    y = staircase(n-2)\n",
    "    z = staircase(n-3)\n",
    "    return x+y+z\n",
    "\n",
    "n = int(input())\n",
    "staircase(n)"
   ]
  }
