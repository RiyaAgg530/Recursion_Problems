    "def issorted(li):\n",
    "    l = len(li)\n",
    "    if l==0 or l==1:\n",
    "        return True\n",
    "    if li[0]>li[1]:\n",
    "        return False\n",
    "    smallOutput = issorted(li[1:])\n",
    "    if smallOutput:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "li = list(int(i) for i in input().split())\n",
    "issorted(li)"
   ]
  }
