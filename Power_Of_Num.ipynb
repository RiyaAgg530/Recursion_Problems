   "source": [
    "def power_of_n(n,x):\n",
    "    if x==0:\n",
    "        return 1\n",
    "    result = n*power_of_n(n,x-1) \n",
    "    return result\n",
    "\n",
    "n= int(input())\n",
    "x= int(input())\n",
    "power_of_n(n,x)"
   ]
