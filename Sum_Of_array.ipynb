    "def sum_of_array(li):\n",
    "    length = len(li) \n",
    "    if length == 1:\n",
    "        return li[0]\n",
    "    else:\n",
    "        return li[0]+sum_of_array(li[1:])\n",
    "    \n",
    "n = int(input())\n",
    "li = list(int(i) for i in input().split())\n",
    "sum_of_array(li)"
   ]
